effect "ImGui/Default"
{
    pass "Default"
    {
        uniform mat4 m_Projection;
        uniform sampler m_Sampler;
        uniform texture2D m_Texture;

        stage vec2 m_Position;
        stage vec2 m_TexCoord;
        stage vec4 m_Color;

        vec4 vert()
        {
            return m_Projection * vec4(m_Position, 0.0, 1.0);
        }

        vec4 frag()
        {
            return m_Color * texture(sampler2D(m_Texture, m_Sampler), m_TexCoord);
        }
    }
}